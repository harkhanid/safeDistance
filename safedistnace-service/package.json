{
  "name": "assignment8",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "scss": "node-sass public/styles/main.scss --output public/dist",
    "start": "http-server -a localhost -p 3000 -c-1",
    "server": "node ./dist/bin/www.js",
    "transpile": "babel ./server --out-dir dist",
    "build": "npm-run-all clean transpile",
    "watch-dev": "nodemon",
    "clean": "rimraf dist",
